
<div class="col-md-12 grid-margin stretch-card" style="margin-top: 150px;">
    <div class="card">
        <div class="card-body">
            <h2 class="card-title">LOGIN Car Admin</h2>
            <form method="post" asp-action="CheckLogin" class="forms-sample">
                <div class="form-group">
                    <label for="exampleInputUsername1">Username</label>
                    <input type="text" name="login" class="form-control" id="exampleInputUsername1" placeholder="Username">
                </div>
                <div class="form-group">
                    <label for="exampleInputPassword1">Password</label>
                    <input type="password" name="pass" class="form-control" id="exampleInputPassword1" placeholder="Password">
                </div>

                <button type="submit" class="btn btn-gradient-primary me-2">Login</button>
                <button class="btn btn-light">Cancel</button>
            </form>
        </div>
    </div>
